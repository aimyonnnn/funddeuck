<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itwillbs.test.mapper.BankMapper">
	<!-- 토큰 정보 저장 -->
	<insert id="insertToken">
		INSERT
			INTO token
			VALUES (
				NULL
				, #{member_idx}
				, #{token.access_token}
				, #{token.token_type}
				, #{token.expires_in}
				, #{token.refresh_token}
				, #{token.scope}
				, #{token.user_seq_no}
			);
	</insert>
</mapper>
