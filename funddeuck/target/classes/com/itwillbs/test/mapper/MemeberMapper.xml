<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itwillbs.test.mapper.MemberMapper">

	<!-- 회원 정보 조회하기 -->
	<select id="selectMember">
		SELECT * 
			FROM members
				WHERE member_id = #{id}
		
	</select>
</mapper>
